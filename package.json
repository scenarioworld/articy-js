{
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/brwarner/articy-node",
  "bugs": "https://github.com/brwarner/articy-node/issues",
  "repository": {
    "url": "https://github.com/brwarner/articy-node",
    "type": "git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "clearMocks": true
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "articy-node",
  "author": "Brook Warner Jensen <brook@scenarioworld.ca>",
  "description": "Loads and allows for navigation of exported Articy JSON data (http://www.articy.com)",
  "module": "dist/articy-node.esm.js",
  "size-limit": [
    {
      "path": "dist/articy-node.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/articy-node.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@size-limit/preset-small-lib": "^4.9.0",
    "@types/uuid": "^8.3.0",
    "husky": "^4.3.0",
    "size-limit": "^4.9.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "uuid": "^8.3.1"
  }
}
